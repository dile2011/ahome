#import "AFamilyLevelController.h"

@implementation AFamilyLevelController

static AFamilyLevelController *sharedInstance = nil;
+ (id)sharedInstance {
    if (sharedInstance == nil) {
        sharedInstance = [[AFamilyLevelController alloc] init];
        sharedInstance.isMenuShow = YES;
    }
    
    return sharedInstance;
}

+ (void)destroyDealloc {
    sharedInstance = nil;
}

- (void)viewDidLoad {
    [super viewDidLoad];
    self.title = self.menuTitle;
    
}

- (instancetype)init {
    self = [super init];
    self.menuTitle = @"家  族";
    self.menuImage = @"Bracket_Home.png";
    
    return self;
}

@end
