#import "ABaseAttriPanelView.h"

@implementation ABaseAttriPanelView

- (instancetype)initWithFrame:(CGRect)frame {
    self = [super initWithFrame:frame];
    
    UIView *titleView = [[UIView alloc]initWithFrame:CGRectMake(.5, .5, frame.size.width-1, 30)];
    [titleView setBackgroundColor:[UIColor colorWithRed:22./255. green:153./255 blue:129./255 alpha:1.]];
    [self addSubview:titleView];
    
    UIImageView *imageView = [[UIImageView alloc]initWithImage:[UIImage imageNamed:@"menu_Attri_bg.png"]];
    [imageView setFrame:CGRectMake(.5, titleView.frame.size.height, titleView.frame.size.width-.5, frame.size.height-titleView.frame.size.height-40)];
    [self addSubview:imageView];
    
    UIView *actionView = [[UIView alloc]initWithFrame:CGRectMake(.5, frame.size.height-40., frame.size.width-1., 40.)];
    [actionView setBackgroundColor:[UIColor blackColor]];
    [self addSubview:actionView];
    
    return self;
}

@end
