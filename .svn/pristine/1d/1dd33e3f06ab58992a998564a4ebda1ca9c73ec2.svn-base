#import "ADynamicLevelController.h"

@implementation ADynamicLevelController

static ADynamicLevelController *sharedInstance = nil;
+ (id)sharedInstance {
    if(sharedInstance == nil) {
        sharedInstance = [[ADynamicLevelController alloc] init];
        sharedInstance.isMenuShow = YES;
    }
    
    return sharedInstance;
}


+ (void)destroyDealloc {
    sharedInstance = nil;
}


- (id)init {
    self = [super init];
    self.menuTitle = @"动  态";
    self.menuImage = @"Bracket_QRCode.png";
    
    return self;
}

- (void)viewDidLoad {
    [super viewDidLoad];
    self.title = self.menuTitle;
}

@end
