#import "AMergeHomeMember.h"

@implementation AMergeHomeMember

- (instancetype)initWithMember:(AHomeMember*)homeMember photo:(NSString*)photo familyId:(int)familyId {
    self = [super init];
    self.placeHolderImage = homeMember.placeHolderImage;
    self.memberState = homeMember.memberState;
    
    self.uid = homeMember.uid;
    self.uname = homeMember.uname;
    self.part = homeMember.part;
    self.phone = homeMember.phone;
    self.avatar = homeMember.avatar;
    
    self.photo = photo;
    self.familyId = familyId;
    
    return self;
}


@end
